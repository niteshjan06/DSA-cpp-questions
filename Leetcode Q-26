class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        if(nums.size() == 1){
            return 1;
        }

        int i=0;
        for(int j=1;j<nums.size();j++){
            if(nums[i] != nums[j]){
                nums[i+1] = nums[j];
                i++;
                j--;
            }
        }
        for(int j=0;nums.size()-i-1;j++){
            nums.pop_back();
        }
        return nums.size();
    }
};
