#include<iostream>
using namespace std;
int main(){
    int n;
    cout << "Enter a number: ";
    cin >> n;

    int j = 1;
    for(int i=1;i<=n;i++){
        int a = 1;
        while(a != i+1){
            cout<<j<<" ";
            j++;
            a++;
        }
        cout<<endl;
    }
    return 0;
}
