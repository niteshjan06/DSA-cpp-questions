class Solution {
public:
    bool isPalindrome(int x) {
        if(x < 0){
            return false;
        }
        int temp = x;
        long long reverse = 0;
        int digit;
        while(temp != 0){
            digit = temp % 10;
            reverse = reverse * 10 + digit;
            temp /= 10;
        }
        if(reverse != x){
            return false;
        }
        return true;
    }
};
