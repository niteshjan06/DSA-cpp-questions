class Solution {
public:
    int majorityElement(vector<int>& nums) {
        map<int,int> mp;

        for(int i=0;i<nums.size();i++){
            mp[nums[i]]++;
        }
    
    int frequent = 0;
    int number = nums[0];
        for(auto i : mp){
            if(frequent < i.second){
                frequent = i.second;
                number = i.first;
            }
        }
        return number;
    }
};
