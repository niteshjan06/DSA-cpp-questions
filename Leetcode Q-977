class Solution {
public:
    vector<int> sortedSquares(vector<int>& nums) {
        int temp;
        for(int &x : nums){
            x = x*x;
        }
        for(int i=0;i<nums.size();i++){
            for(int j=0;j<nums.size()-1;j++){
                if(nums[j] > nums[j+1]){
                    temp = nums[j];
                    nums[j] = nums[j+1];
                    nums[j+1] = temp;
                }
            }
        }
        
        return nums;
    }
};
