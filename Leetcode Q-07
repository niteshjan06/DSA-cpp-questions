class Solution {
public:
    int reverse(int x) {
        long long temp = x;
        long long reverse = 0;
        long long digit;
        while(temp != 0){
            digit = temp % 10;
            reverse = reverse * 10 + digit;
            temp = temp / 10;
        }
        if(reverse > INT_MAX || reverse < INT_MIN){
            return 0;
        }
        return (int)reverse;
    }
};
